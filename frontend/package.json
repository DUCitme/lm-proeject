{
    "name": "vite-react-ts",
    "private": true,
    "version": "0.0.0",
    "scripts": {
        "all": "rm -rf node_modules && rm -rf yarn.lock && yarn install",
        "dev": "set NODE_ENV=development && vite --host",
        "build": "tsc && vite build",
        "preview": "vite preview",
        "lint": "eslint . --ext .ts,tsx ./src/",
        "lint:fix": "eslint --fix --ext .ts,tsx  ./src/",
        "prettier": "prettier --check \"src/**/(*.tsx|*.ts|*.jsx|*.js|*.scss|*.css)\"",
        "prettier:fix": "prettier --write \"src/**/(*.tsx|*.ts|*.jsx|*.js|*.scss|*.css)\"",
        "test": "vitest",
        "coverage": "vitest run --coverage"
    },
    "dependencies": {
        "@chakra-ui/react": "^2.8.2",
        "@emotion/react": "^11.11.3",
        "@emotion/styled": "^11.11.0",
        "@fortawesome/fontawesome-free": "^6.5.1",
        "@fortawesome/fontawesome-svg-core": "^6.5.1",
        "@fortawesome/free-regular-svg-icons": "^6.5.1",
        "@fortawesome/free-solid-svg-icons": "^6.5.1",
        "@fortawesome/react-fontawesome": "^0.2.0",
        "@hookform/error-message": "^2.0.1",
        "@hookform/resolvers": "^3.3.4",
        "@reduxjs/toolkit": "^2.0.1",
        "axios": "^1.6.5",
        "class-transformer": "^0.5.1",
        "class-validator": "^0.14.1",
        "clsx": "^2.1.0",
        "dequal": "^2.0.3",
        "framer-motion": "^10.18.0",
        "fs": "^0.0.1-security",
        "lodash": "^4.17.21",
        "moment": "^2.30.1",
        "nprogress": "^0.2.0",
        "query-string": "^8.1.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-hook-form": "^7.49.3",
        "react-lottie": "^1.2.4",
        "react-redux": "^9.1.0",
        "react-router-dom": "^6.21.3",
        "react-slick": "^0.30.1",
        "react-toastify": "^10.0.3",
        "react-use": "^17.4.3",
        "redux-persist": "^6.0.0",
        "redux-thunk": "^3.1.0",
        "slick-carousel": "^1.8.1"
    },
    "devDependencies": {
        "@types/axios": "^0.14.0",
        "@types/lodash": "^4.14.202",
        "@types/moment": "^2.13.0",
        "@types/node": "^20.11.5",
        "@types/nprogress": "^0.2.3",
        "@types/react": "^18.2.48",
        "@types/react-dom": "^18.2.18",
        "@types/react-lottie": "^1.2.10",
        "@types/react-redux": "^7.1.33",
        "@types/react-router-dom": "^5.3.3",
        "@types/react-slick": "^0.23.13",
        "@types/react-toastify": "^4.1.0",
        "@types/redux-logger": "^3.0.12",
        "@types/redux-persist": "^4.3.1",
        "@typescript-eslint/eslint-plugin": "^6.19.0",
        "@typescript-eslint/parser": "^6.19.0",
        "@typescript-eslint/typescript-estree": "^6.19.0",
        "@vitejs/plugin-react": "^4.2.1",
        "autoprefixer": "^10.4.17",
        "eslint": "^8.56.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-prettier": "^5.1.3",
        "eslint-plugin-react-hooks": "^4.6.0",
        "prettier": "^3.2.4",
        "redux-logger": "^3.0.6",
        "sass": "^1.70.0",
        "tailwindcss": "^3.4.1",
        "typescript": "^5.3.3",
        "vite": "^5.0.12",
        "vite-plugin-imp": "^2.4.0",
        "vite-plugin-inspect": "^0.8.1",
        "vite-plugin-pages": "^0.32.0",
        "vite-plugin-pages-sitemap": "^1.6.1",
        "vite-plugin-windicss": "^1.9.3",
        "vite-tsconfig-paths": "^4.3.1",
        "vitest": "^1.2.1",
        "windicss": "^3.5.6"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version",
            "last 1 ie version"
        ]
    },
    "vite": {
        "optimizeDeps": {
            "include": [
                "@fortawesome/free-solid-svg-icons",
                "@fortawesome/react-fontawesome",
                "@hookform/error-message",
                "@hookform/resolvers/class-validator",
                "@reduxjs/toolkit",
                "axios",
                "class-validator",
                "clsx",
                "dequal",
                "framer-motion",
                "lodash/cloneDeep",
                "react",
                "react-dom",
                "react-hook-form",
                "react-lottie",
                "react-redux",
                "react-router",
                "react-router-dom",
                "react-toastify",
                "react-use",
                "redux-logger",
                "redux-persist",
                "redux-persist/integration/react",
                "redux-persist/lib/storage"
            ]
        }
    }
}
